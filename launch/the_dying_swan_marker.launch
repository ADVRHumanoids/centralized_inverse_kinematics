<!-- This is an example of launch file for ros yarp markers -->
<launch>

     <!-- Launch a node ros_yarp_Cartesian_interactive_marker named ros_yarp_Cartesian_interactive_marker_RArm
     for a task from /Waist to /RSoftHand with a port /bigman/drc_valve/6d::r_arm::Waist/set_ref:i -->
     <node pkg="open_sot" type="ros_yarp_Cartesian_interactive_marker" name="ros_yarp_Cartesian_interactive_marker_RArm" output="screen" >
       <param name="base_link" type="string" value="/world" />
       <param name="distal_link" type="string" value="/RSoftHand" />
       <param name="port_name" type="string" value="/bigman/centralized_inverse_kinematics/cartesian::RSoftHand" />
     </node>

     <node pkg="open_sot" type="ros_yarp_postural_interactive_marker" name="ros_yarp_postural_interactive_marker" output="screen" >
       <param name="urdf_path" type="string" value="$(find bigman_urdf)/urdf/bigman.urdf" />
       <param name="srdf_path" type="string" value="$(find bigman_srdf)/srdf/bigman.srdf" />
       <!--rosparam param="joint_names">["WaistLat", "WaistSag", "WaistYaw", "LShSag", "LShLat", "LShYaw", "LElbj", "RShSag", "RShLat", "RShYaw", "RElbj"]</rosparam-->
       <rosparam param="joint_names">["RShSag"]</rosparam>
       <param name="port_name" type="string" value="/bigman/centralized_inverse_kinematics/Postural" />
     </node>
</launch>
